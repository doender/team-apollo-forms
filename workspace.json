{
  "version": 2,
  "projects": {
    "builder": {
      "root": "apps/builder",
      "sourceRoot": "apps/builder/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/builder",
            "index": "apps/builder/src/index.html",
            "main": "apps/builder/src/index.tsx",
            "polyfills": "apps/builder/src/polyfills.ts",
            "tsConfig": "apps/builder/tsconfig.app.json",
            "assets": [
              "apps/builder/src/favicon.ico",
              "apps/builder/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/builder/src/environments/environment.ts",
                  "with": "apps/builder/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "builder:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "builder:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/builder/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/builder"],
          "options": {
            "jestConfig": "apps/builder/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "builder-e2e": {
      "root": "apps/builder-e2e",
      "sourceRoot": "apps/builder-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/builder-e2e/cypress.json",
            "tsConfig": "apps/builder-e2e/tsconfig.e2e.json",
            "devServerTarget": "builder:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "builder:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/builder-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/core",
            "tsConfig": "libs/core/tsconfig.lib.json",
            "project": "libs/core/package.json",
            "entryFile": "libs/core/src/index.ts",
            "external": [
              "react",
              "react-dom",
              "@emotion/styled",
              "@emotion/react"
            ],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "chakra-ui": {
      "root": "libs/chakra-ui",
      "sourceRoot": "libs/chakra-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/chakra-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/chakra-ui",
            "tsConfig": "libs/chakra-ui/tsconfig.lib.json",
            "project": "libs/chakra-ui/package.json",
            "entryFile": "libs/chakra-ui/src/index.ts",
            "external": [
              "react",
              "react-dom",
              "@emotion/styled",
              "@emotion/react"
            ],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/chakra-ui"],
          "options": {
            "jestConfig": "libs/chakra-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "builder"
}
